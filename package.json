  },
  "dependencies": {
    "has-symbol-support-x": "^1.1.0",
    "is-function-x": "^1.1.0",
    "validate.io-undefined": "^1.0.3"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^11.0.0",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "json3": "^3.3.2",
    "jscs": "^3.0.7",
    "uglify-js": "^2.7.5",
    "browserify": "^14.1.0",
    "browserify-derequire": "^0.9.4",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^3.0.0",
    "nsp": "^2.6.2"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "security": "nsp check",
    "style": "jscs index.js",
    "eslint": "eslint *.js tests/spec/*.js",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "build": "browserify -p browserify-derequire -e index.js -o lib/define-properties-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/define-properties-x.js -o lib/define-properties-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/define-properties-x.map"
  }
